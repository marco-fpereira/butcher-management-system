$ export AWS_ACCESS_KEY_ID="default_access_key"
$ export AWS_SECRET_ACCESS_KEY="default_secret_key"
$ export AWS_DEFAULT_REGION="us-east-1"
$ export aws.dynamodb.endpoint="http://localhost:8000"
$ export aws.dynamodb.region="us-east-1"
$ export aws.dynamodb.accesskey="default_access_key"
$ export aws.dynamodb.secretkey="default_secret_key"